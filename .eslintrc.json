{
  "rules": {
    "no-console": 1,
    "semi": ["error", "always"],
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }]
  },
  "extends": ["next/babel","next/core-web-vitals"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
