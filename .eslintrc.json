{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "next/core-web-vitals",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "error",
        "react-hooks/exhaustive-deps": "off",
        "react/jsx-no-duplicate-props": "off"
    },
    "settings": {
        "react": {
          "version": "detect"
        }
    }
}
